<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta http-equiv="content-type" content="text/html; charset={+IO.encoding+}" />
    <title>JsDoc Reference - Index</title>
    <meta name="generator" content="JsDoc Toolkit" />
    <link rel="stylesheet" href="assets/default.css" />
  </head>

  <body>
    {+include("static/header.html")+}

    <div id="index">
      {+publish.classesIndex+}
    </div>

    <div id="content">
      <h1 class="classTitle">Class Index</h1>

  <%
  var url = request.getURI();
  url = url.substring(0, url.lastIndexOf("/"));

  core.core.file();

  var ignore = ["search.jxp", "DOC_DIR", "index.jxp", "assets"];
  var dir = openFile(url);
  if(dir.exists() && dir.isDirectory()) {
      var f = dir.listFiles();
      for(var i in f) {
          if(!ignore.contains(f[i].filename)) {
              %>
              <div>
                <h2><a href="<%= f[i].filename %>"><%= f[i].filename.substring(0, f[i].filename.lastIndexOf(".")) %></a></h2>
	      </div>
	      <hr />
              <%
          }
      }
  }
  %>

    </div>
    <div class="fineprint" style="clear:both">
      <if test="JSDOC.opt.D.copyright">&copy;{+JSDOC.opt.D.copyright+}<br /></if>
      Documentation generated by <a href="http://www.jsdoctoolkit.org/" target="_blankt">JsDoc Toolkit</a> {+JSDOC.VERSION+} on {+new Date()+}
    </div>
  </body>
</html>
